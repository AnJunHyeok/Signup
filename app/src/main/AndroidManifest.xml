<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="kr.co.kangnam.m2m">

    <application
        android:allowBackup="true"
        android:icon="@drawable/icon"
        android:label="M2M"
        android:roundIcon="@drawable/icon"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.NoActionBar">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".LoginActivity"/>
        <activity android:name=".ResetPassFirstActivity"/>
        <activity android:name=".ResetPassSecondActivity"/>
        <activity android:name=".SignupSelectActivity"/>
        <activity android:name=".AbleSignupFirstActivity"/>
        <activity android:name=".MainPageActivity"/>
        <activity android:name=".MenuActivity"/>
        <activity android:name=".DisableSignupFirstActivity"/>
        <activity android:name=".helpBellRequestActivity"/>
        <activity android:name=".helpBellAccessActivity"/>
        <activity android:name=".helpBellRipleWaitActivity"/>
        <activity android:name=".helpBellRipleCompleteActivity"/>
    </application>
    <uses-permission android:name="android.permission.READ_PHONE_STATE" />
    <uses-permission android:name="android.permission.READ_CONTACTS" />

</manifest>